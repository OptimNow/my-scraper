{
  "version": 2,
  "builds": [
    { "src": "api/scrape_hub.js", "use": "@vercel/node" },
    { "src": "api/save_to_s3.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/scrape_hub", "dest": "api/scrape_hub.js" },
    { "src": "/api/save_to_s3", "dest": "api/save_to_s3.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
